# ============================================
# SEO Audit Platform - Environment Variables
# ============================================

# Database
DB_USER=postgres
DB_PASSWORD=your_secure_password_here
DB_NAME=seoaudit
DATABASE_URL=postgresql://postgres:your_secure_password_here@localhost:5432/seoaudit

# Redis
REDIS_URL=redis://localhost:6379

# Authentication
JWT_SECRET=your_very_long_and_secure_jwt_secret_key_here
JWT_EXPIRES_IN=7d

# OAuth (Google)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Email (Resend)
RESEND_API_KEY=re_your_resend_api_key
EMAIL_FROM=noreply@yourdomain.com

# Stripe Billing
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret
STRIPE_PRICE_PRO=price_your_pro_price_id
STRIPE_PRICE_AGENCY=price_your_agency_price_id

# reCAPTCHA
RECAPTCHA_SITE_KEY=your_recaptcha_site_key
RECAPTCHA_SECRET_KEY=your_recaptcha_secret_key

# Sentry Error Tracking
SENTRY_DSN=https://your_key@sentry.io/project_id

# AWS S3 Storage
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_S3_BUCKET=seo-audit-pdfs
AWS_REGION=us-east-1

# Google PageSpeed API
PAGESPEED_API_KEY=your_pagespeed_api_key

# OpenAI (for AI summaries)
OPENAI_API_KEY=sk-your_openai_api_key

# Application URLs
FRONTEND_URL=http://localhost:3000
BACKEND_URL=http://localhost:4000
NEXT_PUBLIC_API_URL=http://localhost:4000

# Internal Services
SEO_ENGINE_URL=http://localhost:8000
PDF_ENGINE_URL=http://localhost:5000

# Logging
LOG_LEVEL=info
LOG_DIR=logs

# Node Environment
NODE_ENV=development
PORT=4000
